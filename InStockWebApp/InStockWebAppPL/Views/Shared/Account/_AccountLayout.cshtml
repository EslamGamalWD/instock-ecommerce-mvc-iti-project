<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="76x76" href="~/Account/img/apple-icon.png">
    <link rel="icon" type="image/png" href="~/Account/img/favicon.png">
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet"/>
    <!-- Nucleo Icons -->
    <link href="~/Account/css/nucleo-icons.css" rel="stylesheet"/>
    <link href="~/Account/css/nucleo-svg.css" rel="stylesheet"/>
    <!-- Font Awesome Icons -->
    <script src="https://kit.fontawesome.com/42d5adcbca.js" crossorigin="anonymous"></script>
    <!-- CSS Files -->
    <link id="pagestyle" href="~/Account/css/soft-ui-dashboard.css" rel="stylesheet"/>
    @await RenderSectionAsync("HeadSection", false)
    <title>@ViewBag.Title</title>
</head>
<body>

@RenderBody()

@await RenderSectionAsync("scripts", false)
 
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
</body>
</html>