@model SubCategoryVM

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/Admin/_AdminLayout.cshtml";
}

<div class="card">
    <div class="card-header d-flex justify-content-between">
        <div class="header-title">
            <h4 class="card-title">New Subcategory</h4>
        </div>
    </div>
    <div class="card-body">
        <form asp-controller="SubCategory" asp-action="Create" asp-route-subCategoryVm="@Model">
            <partial name="~/Views/Partials/Subcategory/CreateEdit.cshtml"/>
            
        </form>
    </div>
</div>